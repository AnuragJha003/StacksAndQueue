/*for(int i=0;i<n-1;i++){
	        if(arr[i]>arr[i+1]){
	            arr[i]=arr[i+1];
	        }
	        else{
	            arr[i]=-1;
	        }
	    }
	    arr[n-1]=-1;
	        
	    }*/
	    stack<int>s;
	    s.push(-1);//FOR THE LAST ELMNT 
	    for(int i=n-1;i>=0;i--){
	        if(s.top()<arr[i]){
	            int temp=s.top();
	            s.push(arr[i]);
	            arr[i]=temp;
	        }
	        else{
	            s.push(arr[i]);
	            arr[i]=-1;
	        }
	    }
	}

};
